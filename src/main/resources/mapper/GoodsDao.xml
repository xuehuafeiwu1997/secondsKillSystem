<?xml version="1.0" encoding="UTF-8" ?><!DOCTYPE mapper        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"        "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.xmy.secondskill.dao.GoodsDao">    <!--    <resultMap id="goodsVo" type="com.xmy.secondskill.vo.GoodsVo">-->    <!--        <id property="">-->    <!--    </resultMap>-->    <resultMap id="goodsVo" type="com.xmy.secondskill.vo.GoodsVo">        <id property="id" column="id" />        <result property="goodsName" column="goods_name" />        <result property="goodsDetail" column="goods_detail"/>        <result property="goodsStock" column="goods_stock" />        <result property="goodsTitle" column="goods_title" />        <result property="goodsImg" column="goods_img" />        <result property="goodsPrice" column="goods_price" />        <result property="miaoshaPrice" column="miaosha_price" />        <result property="stockCount" column="stock_count" />        <result property="startDate" column="start_date" />        <result property="endDate" column="end_date" />    </resultMap>    <select id="listGoodsVo" resultMap="goodsVo">        select g.*,mg.stock_count, mg.start_date, mg.end_date,mg.miaosha_price from miaosha.miaosha_goods mg left join miaosha.goods g on mg.goods_id = g.id;    </select>    <select id="getGoodsVoByGoodsId" resultMap="goodsVo">        select g.*,mg.stock_count, mg.start_date, mg.end_date,mg.miaosha_price from miaosha.miaosha_goods mg left join miaosha.goods g on mg.goods_id = g.id where g.id = #{goodsId};    </select>    <update id="reduceGoods">        update miaosha_goods set stock_count = stock_count - 1 where goods_id = #{goodsId};    </update>    <select id="getAllGoods" resultType="String">        select  goods_title from goods;    </select>    <resultMap id="goods" type="com.xmy.secondskill.entity.Goods">        <id property="id" column="id" />        <result property="goodsName" column="goods_name" />        <result property="goodsDetail" column="goods_detail"/>        <result property="goodsStock" column="goods_stock" />        <result property="goodsTitle" column="goods_title" />        <result property="goodsImg" column="goods_img" />        <result property="goodsPrice" column="goods_price" />    </resultMap>    <select id="getTestGoods" resultMap="goods">        select  id,goods_name,goods_detail,goods_stock,goods_title,goods_img,goods_price from goods;    </select></mapper>